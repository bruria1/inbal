.not-front #navigation{
  a:after{
      width: 0;
      height: 0;
      border-style: solid;
      border-width: 0 10px 10px 10px;
      border-color: transparent transparent transparent transparent;
      content: "";
      position: relative;
      margin: 0 auto;
      display: block;
  }
    a.active-trail:after, a:hover:after{
      border-color: transparent transparent $light-blue transparent;
      @include res(small-medium){
