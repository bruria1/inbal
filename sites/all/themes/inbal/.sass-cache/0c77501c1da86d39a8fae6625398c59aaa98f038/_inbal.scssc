/**********************************/
/*           mobile menu          */
/**********************************/

body.menu-open #page{
  opacity: 0.7;
}

.region-navigation{
  display: none;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
}

@include res(menu){
  .region-navigation{
    display: block;
    width: 0;
  }
  .region-header{
    display: none;
  }
  #logo{
    margin-top: 16px;
  }
  #main-menu{
    display: none;
  }
  #block-menu-block-6{
    overflow: hidden;
    opacity: 0;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
    padding: 0;
    li{
      width: 100%;
      padding: 0px!important;
      margin: 0;
      transition: all 0.5s;
      -webkit-transition: all 0.5s;
    }
    li li{
      padding: 10px 0!important;
    }
    li.parent.close{
      margin-bottom: -24px;
    }
    a{
      width: 90%;
      display: block;
      padding: 10px!important;
      box-sizing:border-box;
      color: #fff;
      height: 45px;
    }
    a.active{
      padding: 0 10px 0 0!important;
      color:  $gray!important;
    }
    a.active-trail:after, a:hover:after{
      border:none;
    }
    li.close ul{
      max-height: 0;
      overflow: hidden;
    }
    li ul{
      width: 90%;
      float: right;
      transition: all 1s;
      -webkit-transition: all 1s;
      max-height: 800px;
      a{
        padding: 2px 10px 2px !important;
        font-size: 1.1em;
        height: 21px;
        opacity: 0.7;
        display: block;
      }
      li.first{
        a{
          border-top: 1px solid $light-gray;
        }
      }
    }
    li span{
      float: left;
      width: 5%;
      margin-left: 5%;
      height: 22px;
      top:-29px;
      position: relative;
      cursor: pointer;
      background: url("../images/left-arrow.png") no-repeat center center;
      transition: all 0.5s;
      -webkit-transition: all 0.5s;
      @include res(small){
        width: 8%;
        margin-left: 2%;
      }
    }
    li.open span{
      transform: rotate(-90deg);
    }
  }
  .menu-close #block-menu-block-6{
    width: 0;
    padding: 0;
  }
  .menu-open #block-menu-block-6{
    width: 100%;
    background-color: $turkiz;
    opacity: 1;
  }
  .menu-open .region-navigation{
    width: 100%;
  }
}


